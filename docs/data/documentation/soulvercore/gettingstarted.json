ations"}],"identifier":"doc:\/\/app.soulver.core\/documentation\/SoulverCore\/EngineCustomization\/currencyRateProvider","type":"topic","role":"symbol","url":"\/documentation\/soulvercore\/enginecustomization\/currencyrateprovider","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"currencyRateProvider"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"CurrencyRateProvider","preciseIdentifier":"s:11SoulverCore20CurrencyRateProviderP"},{"kind":"text","text":"?"}],"kind":"symbol"},"doc://app.soulver.core/documentation/SoulverCore/Calculator":{"identifier":"doc:\/\/app.soulver.core\/documentation\/SoulverCore\/Calculator","navigatorTitle":[{"text":"Calculator","kind":"identifier"}],"kind":"symbol","type":"topic","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"Calculator","kind":"identifier"}],"url":"\/documentation\/soulvercore\/calculator","role":"symbol","title":"Calculator","abstract":[{"type":"text","text":"Evaluate natural language mathematical expressions"}]},"doc://app.soulver.core/documentation/SoulverCore/FormattingPreferences":{"url":"\/documentation\/soulvercore\/formattingpreferences","role":"symbol","identifier":"doc:\/\/app.soulver.core\/documentation\/SoulverCore\/FormattingPreferences","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"FormattingPreferences","kind":"identifier"}],"title":"FormattingPreferences","abstract":[{"type":"text","text":"A configuration object used to specify how you want SoulverCore to format results"}],"kind":"symbol","navigatorTitle":[{"text":"FormattingPreferences","kind":"identifier"}],"type":"topic"},"https://www.ecb.europa.eu/stats/eurofxref/":{"type":"link","title":"European Central Bank","identifier":"https:\/\/www.ecb.europa.eu\/stats\/eurofxref\/","titleInlineContent":[{"text":"European Central Bank","type":"text"}],"url":"https:\/\/www.ecb.europa.eu\/stats\/eurofxref\/"},"doc://app.soulver